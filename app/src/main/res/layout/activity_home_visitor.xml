<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Visitor.homeVisitor"
    android:background="@color/dark_gray">

    <Button
        android:id="@+id/randomize3"
        android:layout_width="67dp"
        android:layout_height="71dp"
        android:background="@android:color/transparent"
        android:contentDescription="rand"
        app:icon="@drawable/randomize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="0.252" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="sans-serif-black"
        android:text="Select For Me"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/randomize3" />

    <Button
        android:id="@+id/backButton3"
        android:layout_width="59dp"
        android:layout_height="62dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="668dp"
        android:background="@android:color/transparent"
        android:contentDescription="adasda"
        app:icon="@drawable/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="430dp"
        android:layout_height="534dp"
        android:layout_marginTop="75dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@color/darkest_gray"
            android:fontFamily="sans-serif-black"
            android:text="Cuisines"
            android:textAppearance="@style/TextAppearance.AppCompat.Display4"
            android:textSize="30dp" />

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="180dp"
            android:contentDescription="alo">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView24"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView25"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView26"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />
            </LinearLayout>
        </HorizontalScrollView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@color/darkest_gray"
            android:fontFamily="sans-serif-black"
            android:text="Highest Ratings"
            android:textAppearance="@style/TextAppearance.AppCompat.Display4"
            android:textSize="30dp" />

        <HorizontalScrollView
            android:layout_width="wrap_content"
            android:layout_height="180dp"
            android:contentDescription="alo2">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView30"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView31"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView32"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView33"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />

                <ImageView
                    android:id="@+id/imageView34"
                    android:layout_width="172dp"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/image" />
            </LinearLayout>
        </HorizontalScrollView>


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>